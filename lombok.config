#From https://projectlombok.org/features/configuration

#To stop lombok from looking at parent directories for more configuration files, the special key:
config.stopBubbling = true
#Lombok can be configured to add @lombok.Generated annotations to all generated nodes where possible; useful for JaCoCo
#(which has built in support), or other style checkers and code coverage tools:
lombok.addLombokGeneratedAnnotation = true

lombok.log.fieldname = LOG